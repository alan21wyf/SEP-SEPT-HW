@model List<ApplicationCore.Models.FavoriteResponseModel>;

<div class="rounded">
    <div class="container-fluid">
        <div class="row">
            @foreach (var favorite in Model)
            {
                <div class="col-xl-2 col-sm-4 col-lg-3 col-6">
                    <partial name="_FavoriteCard" model="favorite" />
                </div>
            }
        </div>
    </div>
</div>