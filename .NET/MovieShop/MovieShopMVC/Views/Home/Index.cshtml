@model List<ApplicationCore.Models.MovieCardResponseModel>

@{
    ViewData["Title"] = "Home Page";
}

<h2>@ViewBag.PageTitle</h2>
<h3>@ViewData["xyz"]</h3>

<div>
    @foreach (var movie in Model)
    {
        <div>
            <div>
                @movie.Title
            </div>
            <img src ="@movie.PosterUrl"/>
        </div>
    }
</div>